      "cordova-plugin-dbmeter": {},
      "cordova-plugin-brightness": {},
      "cordova-plugin-vibration": {},
      "phonegap-plugin-push": {
        "ANDROID_SUPPORT_V13_VERSION": "27.+",
        "FCM_VERSION": "17.0.+"
      },
